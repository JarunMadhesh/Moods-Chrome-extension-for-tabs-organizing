{
    "name":"Moods",
    "description": "This handles different moods of yours while browsing.",
    "version":"2.0",
    "manifest_version":3,
    "background": {
        "service_worker":"background/background.js"
    },
    "permissions": ["storage", "tabs", "scripting", "bookmarks"],
    "action": {
        "default_popup": "main/index.html"
    },
    "options_page": "background/options.html",
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens main/popup.html"
        }
      },
      "icons": { 
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon64.png",
       "128": "icons/icon128.png" 
      }
}